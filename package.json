{
  "name": "vox-machina",
  "version": "1.0.0",
  "description": "Neural Voice Interface for OpenAI Realtime API testing with cyberpunk-inspired interface",
  "author": "VOX MACHINA Team",
  "license": "MIT",
  "keywords": ["openai", "realtime-api", "voice-ai", "webrtc", "cyberpunk", "neural-interface"],
  "type": "module",
  "scripts": {
    "dev": "node server.js --dev",
    "start": "node server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr /index.js",
    "lint": "eslint . --ext .js,.jsx --fix",
    "test": "vitest run",
    "test:client": "vitest --config vitest.config.js run",
    "test:server": "vitest --config vitest.server.config.js run",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "history": "^5.3.0",
    "localtunnel": "^2.0.2",
    "minipass": "^7.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "postcss": "^8.4.31",
    "postcss-nesting": "^12.0.2",
    "postcss-preset-env": "^7.7.1",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.2",
    "vitest": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "jsdom": "^24.0.0",
    "@testing-library/react": "^14.2.1",
    "@testing-library/jest-dom": "^6.4.2",
    "whatwg-fetch": "^3.6.20",
    "supertest": "^7.0.0"
  }
}
